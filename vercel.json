{
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/index.html$",
      "dest": "/index.html"
    },
    {
      "src": "^/chi-sono$",
      "dest": "/chi-sono.html"
    },
    {
      "src": "^/chi-sono.html$",
      "dest": "/chi-sono.html"
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/api/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/$1"
    }
  ]
}