{
  "version": 2,
  "buildCommand": "echo 'Build complete'",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/ebooks",
      "dest": "/ebooks.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}